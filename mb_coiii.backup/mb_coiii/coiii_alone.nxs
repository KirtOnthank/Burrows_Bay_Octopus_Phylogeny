begin mrbayes;
 
      charset coiii_pos1 =  1 - 664\3;
      charset coiii_pos2 =  2 - 664\3;
      charset coiii_pos3 =  3 - 664\3;
 
      partition by_gene = 3:coiii_pos1,coiii_pos2,coiii_pos3;
	    set partition = by_gene;

	    constraint outg = Octopus_vulgaris Octopus_rubescens Octopus_bimaculoides Octopus_cyanea Amphioctopus_aegina Hapalochlaena_maculosa Abdopus_aculeatus Octopus_tetricus Octopus_berrima Macroctopus_maorum;
	    prset topologypr = constraints (outg);
	    outgroup Octopus_vulgaris;
	    
	    lset applyto=(all)   nucmodel=4by4;
	    prset ratepr=variable;
	    unlink statefreq=(all) revmat=(all) shape=(all) pinvar=(all);
      lset applyto=(1) nst=6 rates=invgamma;
      lset applyto=(2) nst=2 rates=invgamma;
      lset applyto=(3) nst=6 rates=invgamma;

      mcmcp ngen=100000000 printfreq=1000 samplefreq=10000 stoprule=yes stopval=0.01 nruns=5 nchains=2 burninfrac=0.25;
      mcmc;
      sump;
      sumt filename=mb_coiii/coiii_alone.nxs contype=allcompat conformat=simple;

end;

